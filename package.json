{
  "name": "three",
  "version": "0.82.1",
  "description": "JavaScript 3D library",
  "main": "build/three.js",
  "repository": "mrdoob/three.js",
  "jsnext:main": "build/three.modules.js",
  "files": [
    "LICENSE",
    "README.md",
    "build/three.js",
    "build/three.min.js",
    "build/three.modules.js",
    "src",
    "examples/js",
    "examples/fonts",
    "examples/textures/perlin-512.png",
    "examples/textures/sprite0.png",
    "examples/textures/waternormals.jpg",
    "examples/textures/UV_Grid_Sm.jpg",
    "examples/textures/grid.png",
    "examples/textures/hardwood2_bump.jpg",
    "examples/textures/hardwood2_diffuse.jpg",
    "examples/textures/hardwood2_roughness.jpg",
    "examples/textures/brick_bump.jpg",
    "examples/textures/brick_diffuse.jpg",
    "examples/textures/brick_roughness.jpg",
    "examples/textures/cube/skybox",
    "examples/textures/cube/sun_temple_stripe_stereo.jpg",
    "examples/textures/sprites",
    "examples/models/obj/vive-controller",
    "utils/build",
    "rollup.config.js"
  ],
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c",
    "build-uglify": "rollup -c && uglifyjs build/three.js -cm --preamble \"// threejs.org/license\" > build/three.min.js",
    "build-closure": "rollup -c && java -jar utils/build/compiler/closure-compiler-v20160713.jar --warning_level=VERBOSE --jscomp_off=globalThis --jscomp_off=checkTypes --externs utils/build/externs.js --language_in=ECMASCRIPT5_STRICT --js build/three.js --js_output_file build/three.min.js",
    "dev": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "three",
    "three.js",
    "3d",
    "webgl"
  ],
  "author": "mrdoob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrdoob/three.js/issues"
  },
  "homepage": "http://threejs.org/",
  "devDependencies": {
    "argparse": "^1.0.3",
    "jscs": "^1.13.1",
    "rollup": "^0.36.3",
    "rollup-watch": "^2.5.0",
    "uglify-js": "^2.6.0"
  }
}
